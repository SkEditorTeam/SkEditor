<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SkEditor.Controls.Docs.DocManagementEntry">
    <Grid ColumnDefinitions="Auto,*,Auto">
        
        <TextBlock VerticalAlignment="Center" TextAlignment="Left" Grid.Column="0" FontWeight="SemiBold" Name="NameBlock" />
        <TextBlock VerticalAlignment="Center" TextAlignment="Right" Margin="0,0,5,0" Grid.Column="1" Name="Description" />
        
        <Button Grid.Column="2" Name="DeleteButton" Content="{DynamicResource MenuHeaderDelete}" />
        
    </Grid>
</UserControl>
